    "react-date-picker": "^7.10.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.9.0",
    "react-select": "^3.0.8",
    "react-share": "^4.1.0",
    "react-spring": "^8.0.27",
    "react-with-direction": "^1.3.1",
    "typeface-work-sans": "^0.0.72",
